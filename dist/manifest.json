{
  "name": "Tinkoff Business notifier",
  "version": "1.3.0",
  "manifest_version": 2,
  "description": "Be notified for every transaction!",
  "homepage_url": "https://chrome.google.com/webstore/detail/unofficial-tinkoff-busine/onjegelkkddpepjondhmpaekjbkifaem",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "bg/bundle.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Open Tinkoff Business"
  },
  "permissions": [
    "tabs",
    "cookies",
    "notifications",
    "https://sme.tinkoff.ru/*",
    "https://business.tinkoff.ru/*"
  ]
}